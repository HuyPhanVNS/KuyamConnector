<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.17.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ui.datepicker.js")" type="text/javascript"></script>
    
</head>
<body>
    <div class="page">
       <div id="header">
            <div id="title">
                <h1>KUYAM SAMPLE SITE: Current user id: @HttpContext.Current.Session["UserId"]</h1>
            </div>

           @* <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>*@
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("Facebook connector", "fbCalendar")</li>
                    <li>@Html.ActionLink("Google connector", "ggCalendar")</li>
                    <li>@Html.ActionLink("Upload iCalendar", "iCalendar")</li>
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
        </div>
    </div>
</body>
</html>
